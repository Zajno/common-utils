{
  "name": "@zajno/common",
  "version": "2.7.3",
  "description": "Zajno's re-usable utilities for JS/TS projects",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zajno/common-utils.git"
  },
  "author": "Zajno <hello@zajno.com> (https://zajno.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zajno/common-utils/issues"
  },
  "homepage": "https://github.com/Zajno/common-utils/tree/main/packages/common#readme",
  "scripts": {
    "clean": "scripty",
    "dev": "scripty",
    "build": "scripty",
    "build:clean": "scripty",
    "build:w": "scripty",
    "publish:local": "scripty",
    "publish:from-dist": "scripty",
    "build:full": "scripty",
    "bundle": "scripty",
    "update-exports": "scripty",
    "lint": "scripty",
    "test": "scripty"
  },
  "config": {
    "scripty": {
      "path": "../scripts"
    }
  },
  "devDependencies": {
    "@zajno/eslint-config": "*"
  },
  "peerDependencies": {
    "tslib": "^2.8"
  },
  "files": [
    "LICENSE",
    "!.DS_Store",
    "!*.tsbuildinfo",
    "*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./*": "./src/*.ts",
    "./api": "./src/api/index.ts",
    "./api/extensions": "./src/api/extensions/index.ts",
    "./async": "./src/async/index.ts",
    "./dates": "./src/dates/index.ts",
    "./fields": "./src/fields/index.ts",
    "./functions": "./src/functions/index.ts",
    "./lazy": "./src/lazy/index.ts",
    "./localization": "./src/localization/index.ts",
    "./logger": "./src/logger/index.ts",
    "./logger/buffered": "./src/logger/buffered/index.ts",
    "./logger/buffered/file": "./src/logger/buffered/file/index.ts",
    "./math": "./src/math/index.ts",
    "./math/object": "./src/math/object/index.ts",
    "./storage": "./src/storage/index.ts",
    "./structures/path": "./src/structures/path/index.ts",
    "./types": "./src/types/index.ts",
    "./validation": "./src/validation/index.ts"
  }
}
