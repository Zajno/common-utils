{
  "name": "@zajno/common-mobx",
  "version": "1.1.2",
  "description": "Zajno's re-usable utilities for JS/TS projects using MobX",
  "main": "index.js",
  "author": "Zajno <hello@zajno.com> (https://zajno.com)",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zajno/common-mobx.git"
  },
  "bugs": {
    "url": "https://github.com/Zajno/common-mobx/issues"
  },
  "homepage": "https://github.com/Zajno/common-mobx#readme",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build:clean": "rimraf ./dist tsconfig.build.tsbuildinfo && yarn build",
    "build:w": "yarn build --watch",
    "publish:local": "yarn build-for-publish && yarn copy-for-publish && cd dist && yalc push --replace --update",
    "publish:from-dist": "yarn build-for-publish && yarn copy-for-publish && cd dist && yarn publish",
    "build-for-publish": "yarn build:clean && yarn lint && yarn test",
    "copy-for-publish": "cpy --flat ../../LICENSE package.json README.md dist/",
    "lint": "eslint \"src/**/*.ts?(x)\"",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^18.19.2",
    "@zajno/common": "^2.1.3",
    "@zajno/eslint-config": "^3.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.55.0",
    "mobx": "^6.12.0",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.1"
  },
  "peerDependencies": {
    "@zajno/common": "^2.1.2",
    "mobx": "^6.10",
    "tslib": "^2.6"
  },
  "files": [
    "*",
    "LICENSE",
    "!.DS_Store"
  ],
  "publishConfig": {
    "access": "public"
  }
}
