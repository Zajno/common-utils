{
  "name": "@zajno/common",
  "version": "1.3.16",
  "description": "Zajno's re-usable utilities for JS/TS projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zajno/common-utils.git"
  },
  "author": "Zajno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zajno/common-utils/issues"
  },
  "homepage": "https://github.com/Zajno/common-utils#readme",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "build:w": "yarn build --watch",
    "publish:local": "yalc push --replace --update",
    "prepare": "yarn build && yarn lint && yarn test",
    "postinstall": "yarn build",
    "lint": "eslint \"src/**/*.ts?(x)\"",
    "test": "yarn jest --clearCache && yarn jest"
  },
  "dependencies": {
    "rimraf": "^3"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.3",
    "@zajno/eslint-config": "^2.0.0",
    "eslint": "^8.0.1",
    "faker": "^5.5.3",
    "fast-check": "^2.18.0",
    "jest": "^27.3.1",
    "mobx": "^6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "tslib": "^2.2.0",
    "tslint": "^6.1.3",
    "typescript": "4"
  },
  "peerDependencies": {
    "mobx": "^6",
    "tslib": "^2",
    "typescript": "^4"
  },
  "files": [
    "src/*",
    "tsconfig.json",
    "LICENSE",
    "!.DS_Store",
    "!**/__tests__/**/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
