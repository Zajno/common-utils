{
  "name": "@zajno/common",
  "version": "2.0.6",
  "description": "Zajno's re-usable utilities for JS/TS projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zajno/common-utils.git"
  },
  "author": "Zajno <hello@zajno.com> (https://zajno.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zajno/common-utils/issues"
  },
  "homepage": "https://github.com/Zajno/common-utils#readme",
  "scripts": {
    "build": "tsc -P tsconfig.build.json",
    "build:clean": "rimraf ./lib && yarn build",
    "build:w": "yarn build --watch",
    "publish:local": "yarn build-for-publish && yarn copy-for-publish && cd lib && yalc push --replace --update",
    "publish:from-lib": "yarn build-for-publish && yarn copy-for-publish && cd lib && yarn publish",
    "build-for-publish": "yarn build:clean && yarn lint && yarn test",
    "copy-for-publish": "cpy package.json LICENSE README.md lib/",
    "lint": "eslint \"src/**/*.ts?(x)\"",
    "test": "yarn jest --clearCache && yarn jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.7",
    "@zajno/eslint-config": "^2.2.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.26.0",
    "fast-check": "^3.3.0",
    "jest": "^29.2.2",
    "jest-extended": "^3.1.0",
    "rimraf": "^3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "tslib": "^2.4"
  },
  "files": [
    "LICENSE",
    "!.DS_Store",
    "*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
