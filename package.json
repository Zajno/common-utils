{
  "name": "@zajno/common",
  "version": "1.6.4",
  "description": "Zajno's re-usable utilities for JS/TS projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zajno/common-utils.git"
  },
  "author": "Zajno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zajno/common-utils/issues"
  },
  "homepage": "https://github.com/Zajno/common-utils#readme",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "build:w": "yarn build --watch",
    "publish:local": "yalc push --replace --update",
    "prepare": "yarn build && yarn lint && yarn test",
    "postinstall": "yarn build",
    "lint": "eslint \"src/**/*.ts?(x)\"",
    "test": "yarn jest --clearCache && yarn jest"
  },
  "dependencies": {
    "rimraf": "^3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^16.11.59",
    "@zajno/eslint-config": "^2.2.0",
    "eslint": "^8.23.1",
    "fast-check": "^3.1.4",
    "jest": "^29.0.3",
    "jest-extended": "^3.1.0",
    "mobx": "^6.6.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "mobx": "^6.6",
    "tslib": "^2.4",
    "typescript": "^4.8"
  },
  "files": [
    "src/*",
    "tsconfig.json",
    "LICENSE",
    "!.DS_Store",
    "!**/__tests__/**/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
